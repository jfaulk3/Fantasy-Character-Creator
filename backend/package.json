{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node build/server.js",
    "migrate": "knex migrate:latest --knexfile ./knexfile.ts",
    "seed": "knex seed:run --knexfile ./knexfile.ts",
    "dev": "ts-node-dev --poll --no-notify build/server.js",
    "knex:migrate:make": "knex --knexfile src/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex --knexfile src/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/knexfile.ts migrate:rollback"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "common-js": "^0.3.8",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.3.0"
  }
}
